{
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "app/prettier.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/main.py"
    },
    {
      "src": "/prettier",
      "dest": "/app/prettier.js"
    }
  ]
}